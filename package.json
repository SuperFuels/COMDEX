{
  "name": "comdex",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "build": "cd frontend && next build",
    "start": "cd frontend && next start -p 3000",
    "dev": "concurrently \"cd frontend && next dev\" \"npm run collab\" \"npm run kg\"",
    "collab": "ts-node --transpile-only -P tsconfig.collab.json server/collab-server.ts",
    "lint": "cd frontend && next lint",
    "preview": "npm run build && npm run start",
    "kg": "ts-node --transpile-only -P server/tsconfig.json server/kg.ts",
    "dev:server": "ts-node -P server/tsconfig.json server/kg.ts",
    "build:server": "tsc -p server/tsconfig.json",
    "start:server": "node server/dist/kg.js",
    "postinstall": "pnpm rebuild better-sqlite3 esbuild || true"
  },
  "dependencies": {
    "3d-force-graph": "^1.73.0",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-spring/three": "10.0.1",
    "@react-three/drei": "9.56.26",
    "@react-three/fiber": "8.15.16",
    "@tailwindcss/forms": "^0.5.10",
    "@tweenjs/tween.js": "^25.0.0",
    "@typescript-eslint/visitor-keys": "8.34.0",
    "axios": "^1.13.1",
    "better-sqlite3": "^12.4.1",
    "concurrently": "^8.2.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "ethers": "^5.8.0",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.0",
    "jspdf": "^3.0.3",
    "lightweight-charts": "4.0.0",
    "lucide-react": "^0.525.0",
    "monaco-editor": "^0.52.2",
    "nanoid": "^5.1.5",
    "next": "^15.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-force-graph": "^1.48.0",
    "react-force-graph-3d": "^1.28.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-pdf": "^10.1.0",
    "react-router-dom": "^6.30.2",
    "react-select": "^5.10.2",
    "react-split": "^2.0.14",
    "recharts": "^3.2.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "0.154.0",
    "ws": "^8.18.3",
    "y-websocket": "^1.5.4",
    "yjs": "^13.6.27",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.5",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "0.154.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "5.4.21"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}