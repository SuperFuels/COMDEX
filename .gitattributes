node_modules/@next/swc-darwin-arm64/next-swc.darwin-arm64.node filter=lfs diff=lfs merge=lfs -text

# Treat all knowledge capsules as binary (no text diff, no merge)
*.wiki.phn binary
*.qkg.json binary
*.log.json binary
*.tar.gz binary
*.zip binary
*.db binary

# Normalize line endings for code
*.py text eol=lf
*.js text eol=lf
*.ts text eol=lf
*.md text eol=lf
*.yaml text eol=lf
*.yml text eol=lf

# Keep JSONs consistent
*.json text eol=lf diff=json

# Ignore whitespace changes in diffs for these
*.py diff=python
*.js diff=javascript

*.photon filter=photon

photon_src/*.py filter=photonlangphoton_src/*.py filter=photonlang
