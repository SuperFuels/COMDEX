{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AtomSheet",
  "type": "object",
  "required": ["cells"],
  "properties": {
    "metadata": {
      "type": "object",
      "description": "Optional metadata for the AtomSheet"
    },
    "cells": {
      "type": "array",
      "items": {
        "$ref": "glyph_cell_schema.json"
      },
      "description": "List of GlyphCells comprising this AtomSheet"
    }
  },
  "additionalProperties": false
}